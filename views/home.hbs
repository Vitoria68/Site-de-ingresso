<div class="showsDisponíveis">
    <h1 class="display-4">Shows Disponíveis</h1>
    <div class="container-fluid cardsInter">
        <div class="row g-3 justify-content-center">
            {{#each ListaDeShows}}
            <div class="col-sm-12 col-md-4">
                <div class="card h-100">
                    <img src="../img/{{imagem}}" class="card-img-top" alt="{{nome}}">
                    <div class="card-body">
                        <h5 class="card-title">{{nome}}</h5>
                        <p class="card-text">{{data}}</p>
                        <a href="/show/consulta/{{id}}"><button type="button" class="btn btn-dark">Comprar Ingresso</button></a>
                    </div>
                </div>
            </div>
            {{/each}}
        </div>
    </div>
</div>